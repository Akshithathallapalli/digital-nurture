1.Setting Up Junit
public class calculator
{
  public int add(int i,int j)  
  {
    return i+j;
   }
}
Test
import static org.Junit.Assert.*;
import org.junit.Test;
public class TestCalculator
{
  Calculator c=new Calculator();
  @Test
  public void testAdd()
 {
    assertEquals(5,c.add(2,3));
 }
}


